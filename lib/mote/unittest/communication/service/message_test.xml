<awap-communication version="0.1" module="service.message_test">
<import module="common" as="" />

<service name="MessageTestService">
	<messages>
		<message name="EmptyMessage" direction="rx" performative="INFORM" id="0" />
		<message name="SimpleIntMessage" direction="rx" performative="INFORM" id="1" >
			<int name="int_value" id="0" />
		</message>
		<message name="SimpleShortMessage" direction="rx" performative="INFORM" id="2" >
			<short name="short_value" id="0" />
		</message>
		<message name="EnumMessage" direction="rx" performative="INFORM" id="3" >
			<enum name="enum_value" class="Building" id="0" />
		</message>
		<message name="BoolMessage" direction="rx" performative="INFORM" id="4" >
			<bool name="b0" id="0" />
			<bool name="b1" id="1" />
			<bool name="b2" id="2" />
		</message>
	</messages>
</service>
</awap-communication>
